package NestedLoops.Lab;

public class multiplicationTable {
    public static void main(String[] args) {
        for (int i = 1; i <= 10; i++){
            for (int j = 1; j<=10; j++){
                if (j > 10){
                    j = 1;
                }
                int k = i * j;
                System.out.printf("%d * %d = %d%n", i, j, k);
            }
        }
    }
}
