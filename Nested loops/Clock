package NestedLoops.Lab;

public class clock {
    public static void main(String[] args) {
        for (int i = 0; i <= 23; i++){
            for (int j = 0; j <= 59; j++){
                if (j > 59){
                    j = 0;
                }
                System.out.printf("%d:%d%n", i,j);
            }
        }
    }
}
