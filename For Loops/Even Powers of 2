package ForLoop.Lab;

import java.util.Scanner;

public class EvenPowersOf2 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int m = input.nextInt();
        double sum;

        for (double i = 0; i <= m; i +=2) {
            sum = Math.pow(2, i);
            System.out.printf("%.0f%n", sum);
        }
    }
}
